{
    "name": "akbardwi/laravel-env-editor",
    "description": "The .env file editor tool for Laravel 5.8+",
    "license": "MIT",
    "keywords": ["laravel", "dotenv", "laravel-dotenv-editor"],
    "authors": [
        {
            "name": "Akbar Dwi Syahputra",
            "email": "admin@mail.akbardwi.my.id"
        }
    ],
    "require": {
        "illuminate/console": "^11.0|^10.0|^9.0|^8.0|^7.0|^6.0|^5.8",
        "illuminate/contracts": "^11.0|^10.0|^9.0|^8.0|^7.0|^6.0|^5.8",
        "illuminate/support": "^11.0|^10.0|^9.0|^8.0|^7.0|^6.0|^5.8",
        "Akbardwi/path-helper": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Akbardwi\\LaravelEnvEditor\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Akbardwi\\LaravelEnvEditor\\LaravelEnvEditorServiceProvider"
            ],
            "aliases": {
                "LaravelEnvEditor": "Akbardwi\\LaravelEnvEditor\\Facades\\LaravelEnvEditor"
            }
        }
    },
    "minimum-stability": "stable"
}
